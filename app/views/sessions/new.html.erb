<h1>Log in</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag 'Last Name' %><br />
    <%= text_field_tag :last_name, params[:last_name] %>
  </p>
  <p>
    <%= label_tag 'Membership Number' %><br />
    <%= password_field_tag :membership_number %>
  </p>
  <p class="button"><%= submit_tag 'Login' %></p>
<% end %>